{
  "name": "core",
  "packageManager": "yarn@3.0.2",
  "scripts": {
    "lint": "eslint .",
    "clean": "rm -rf build",
    "compile": "tsc",
    "fix": "eslint . && prettier --write --ignore-path .eslintignore **/*.json **/.*.js",
    "prepublish": "yarn clean && yarn compile",
    "pretest": "yarn compile",
    "posttest": "yarn lint"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "eslint": "^7.32.0",
    "prettier": "^2.4.1",
    "sijiaoh-gts": "^0.4.0",
    "typescript": "^4.4.3"
  }
}
